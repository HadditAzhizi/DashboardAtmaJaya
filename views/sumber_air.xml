<odoo>
  <data> 
    <!-- Client Tree View -->
    <record model="ir.ui.view" id="dashboard_web_sa_tree">
      <field name="name">dashboard_web.sa_tree</field>
      <field name="model">dashboard_web.sumber_air</field>
      <field name="arch" type="xml">
        <tree>
          <field name="provinsi"/>
          <field name="jenis"/>
          <field name="kategori"/>
          <field name="persentase"/>
        </tree>
      </field>
    </record>

    <!-- Client Form View -->
    <record model="ir.ui.view" id="dashboard_web_sa_form">
      <field name="name">dashboard_web.sa_form</field>
      <field name="model">dashboard_web.sumber_air</field>
      <field name="arch" type="xml">
        <form string="Room">
          <sheet>
            <group>
                <field name="provinsi"/>
                <field name="jenis"/>
            </group>
            <group>
                <field name="kategori"/>
                <field name="persentase"/>
            </group>
          </sheet>
        </form>
      </field>
    </record>
    <!-- Client Action Window -->
    <record model="ir.actions.act_window" id="dashboard_web_sumber_air_action_window">
      <field name="name">Sumber Air</field>
      <field name="res_model">dashboard_web.sumber_air</field>
      <field name="view_mode">tree,form</field>
    </record>

    <menuitem name="Dashboard Web" id="dashboard_web.root_menu" />

    <menuitem name="Master Data" id="dashboard_web.master_data" parent="dashboard_web.root_menu" />

    <menuitem name="Survey" id="dashboard_web.survey" parent="dashboard_web.root_menu" />

      <!-- Menu Item -->
    <menuitem name="Sumber Air" id="dashboard_web.sumber_air"  parent="dashboard_web.survey" action="dashboard_web_sumber_air_action_window"  sequence="1"/>
<!--     <menuitem name="Sumber Penerangan Utama" id="dashboard_web.sumber_air"  parent="dashboard_web.survey" action="dashboard_web_sumber_pu_action_window"  sequence="2"/>
    <menuitem name="Bahan Bakar Cadangan" id="dashboard_web.sumber_air"  parent="dashboard_web.survey" action="dashboard_web_sumber_bc_action_window"  sequence="3"/>
    <menuitem name="Bahan Bakar Utama" id="dashboard_web.sumber_air"  parent="dashboard_web.survey" action="dashboard_web_sumber_by_action_window"  sequence="4"/>
    <menuitem name="Pengetahuan Perilaku Lingkungan Hidup" id="dashboard_web.sumber_air"  parent="dashboard_web.survey" action="dashboard_web_plh_action_window"  sequence="5"/> -->
  </data>
</odoo>