<odoo>
  <data>  
    <!-- Client Tree View -->
    <record model="ir.ui.view" id="dashboard_web_perlakuan_uts_tree">
      <field name="name">dashboard_web.perlakuan_uts_tree</field>
      <field name="model">dashboard_web.perlakuan_uts</field>
      <field name="arch" type="xml">
        <tree>
          <field name="provinsi"/>
          <field name="jenis"/>
          <field name="kategori"/>
          <field name="persentase"/>
        </tree>
      </field>
    </record>

    <!-- Client Form View -->
    <record model="ir.ui.view" id="dashboard_web_perlakuan_uts_form">
      <field name="name">dashboard_web.perlakuan_uts_form</field>
      <field name="model">dashboard_web.perlakuan_uts</field>
      <field name="arch" type="xml">
        <form string="Room">
          <sheet>
            <group>
                <field name="provinsi"/>
                <field name="jenis"/>
            </group>
            <group>
                <field name="kategori"/>
                <field name="persentase"/>
            </group>
          </sheet>
        </form>
      </field>
    </record>
    <!-- Client Action Window -->
    <record model="ir.actions.act_window" id="dashboard_web_perlakuan_uts_action_window">
      <field name="name">Perlakuan Utama Terhadap Sampah</field>
      <field name="res_model">dashboard_web.perlakuan_uts</field>
      <field name="view_mode">tree,form</field>
    </record>

    <menuitem name="Dashboard Web" id="dashboard_web.root_menu" />

    <menuitem name="Master Data" id="dashboard_web.master_data" parent="dashboard_web.root_menu" />

    <menuitem name="Survey" id="dashboard_web.survey" parent="dashboard_web.root_menu" />

      <!-- Menu Item -->
    <menuitem name="Perlakuan Utama Terhadap Sampah" id="dashboard_web.perlakuan_uts"  parent="dashboard_web.survey" action="dashboard_web_perlakuan_uts_action_window"  sequence="5"/>
  </data>
</odoo>