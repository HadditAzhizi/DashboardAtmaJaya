<odoo>
  <data>  
    <!-- Client Tree View -->
    <record model="ir.ui.view" id="dashboard_web_bb_utama_tree">
      <field name="name">dashboard_web.bb_utama_tree</field>
      <field name="model">dashboard_web.bb_utama</field>
      <field name="arch" type="xml">
        <tree>
          <field name="provinsi"/>
          <field name="jenis"/>
          <field name="kategori"/>
          <field name="persentase"/>
        </tree>
      </field>
    </record>

    <!-- Client Form View -->
    <record model="ir.ui.view" id="dashboard_web_bb_utama_form">
      <field name="name">dashboard_web.bb_utama_form</field>
      <field name="model">dashboard_web.bb_utama</field>
      <field name="arch" type="xml">
        <form string="Room">
          <sheet>
            <group>
                <field name="provinsi"/>
                <field name="jenis"/>
            </group>
            <group>
                <field name="kategori"/>
                <field name="persentase"/>
            </group>
          </sheet>
        </form>
      </field>
    </record>
    <!-- Client Action Window -->
    <record model="ir.actions.act_window" id="dashboard_web_bb_utama_action_window">
      <field name="name">Bahan Bakar Utama</field>
      <field name="res_model">dashboard_web.bb_utama</field>
      <field name="view_mode">tree,form</field>
    </record>

    <menuitem name="Dashboard Web" id="dashboard_web.root_menu" />

    <menuitem name="Master Data" id="dashboard_web.master_data" parent="dashboard_web.root_menu" />

    <menuitem name="Survey" id="dashboard_web.survey" parent="dashboard_web.root_menu" />

      <!-- Menu Item -->
    <menuitem name="Bahan Bakar Utama" id="dashboard_web.bb_utama"  parent="dashboard_web.survey" action="dashboard_web_bb_utama_action_window"  sequence="2"/>
  </data>
</odoo>