<odoo> 
    <template id="assets_editor" inherit_id="web.assets_frontend" name="Dashboard web">
      <xpath expr="." position="inside">
          <script type="text/javascript" src="/dashboard_web/static/src/js/data_map.js"></script>
      </xpath>
    </template>
    
    <template id="index"> 
      <t t-call="website.layout">
          <t t-set="title">Atma Jaya - Dashboard</t>
          <style>
              .formRight select {
                  background: none repeat scroll 0 0 #FFFFFF;
                  border: 1px solid #E5E5E5;
                  border-radius: 25px 25px 25px 25px;
                  box-shadow: 0 0 10px #e8e8e8 inset;
                  height: 35px;
                  margin: 0 0 0 25px;
                  padding: 8px;
                  padding-top: 4px;
                  width: 110px; 
              }
              .box{
                box-sizing: border-box;
                border: 3px solid transparent;
                background-clip:padding-box;
              }
              #chartdiv {
                width: 100%;
                height: 220px;
              }
              .head_conf
              {
                  color:white !important;
              }
              .head_conf .col-md-12
              {
                  margin-top:5px;
              }
              .head_conf .head_title
              {
                  color:white !important;
              }
              .head_conf hr
              {
                  color: white;
                  border-top: 1px solid #f6f6f6;
              }

              .switch_input {
                  top:5px;
                  position: relative;
                  display: inline-block;
                  width: 30px;
                  height: 14px;
              }

              /* Hide default HTML checkbox */
              .switch_input input {
                opacity: 0;
                width: 0;
                height: 0;
              }

              /* The slider_input */
              .slider_input {
                position: absolute;
                cursor: pointer;
                top: 0;
                left: 0;
                right: 0;
                bottom: 0;
                background-color: white;
                -webkit-transition: .4s;
                transition: .4s;
              }

              .slider_input:before {
                  position: absolute;
                  content: "";
                  height: 10px;
                  width: 9px;
                  left: 4px;
                  bottom: 2px;
                  background-color: #707070;
                  -webkit-transition: .4s;
                  transition: .4s;
              }

              input:checked + .slider_input {
                background-color: white;
              }

              input:focus + .slider_input {
                box-shadow: 0 0 1px white;
              }

              input:checked + .slider_input:before {
                -webkit-transform: translateX(26px);
                -ms-transform: translateX(26px);
                transform: translateX(26px);
                left: -10px;
              background-color: #E97118;
              }

              /* Rounded slider_inputs */
              .slider_input.round {
                border-radius: 34px;
              }

              .slider_input.round:before {
                border-radius: 50%;
              }
          </style>
