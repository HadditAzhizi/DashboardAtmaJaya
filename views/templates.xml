                }
                .divdashboard  {
                    font-family: Quicksand !important;
                }
                h1, h2, h3, h4, h5, h6 
                {
                    font-family: Quicksand !important;
                }
                /* The switch_input - the box around the slider_input */
                .switch_input {
                    top:5px;
                    position: relative;
                    display: inline-block;
                    width: 30px;
                    height: 14px;
                }

                /* Hide default HTML checkbox */
                .switch_input input {
                  opacity: 0;
                  width: 0;
                  height: 0;
                }

                /* The slider_input */
                .slider_input {
                  position: absolute;
                  cursor: pointer;
                  top: 0;
                  left: 0;
                  right: 0;
                  bottom: 0;
                  background-color: white;
                  -webkit-transition: .4s;
                  transition: .4s;
                }

                .slider_input:before {
                    position: absolute;
                    content: "";
                    height: 10px;
                    width: 9px;
                    left: 4px;
                    bottom: 2px;
                    background-color: #707070;
                    -webkit-transition: .4s;
                    transition: .4s;
                }

                input:checked + .slider_input {
                  background-color: white;
                }

                input:focus + .slider_input {
                  box-shadow: 0 0 1px white;
                }

                input:checked + .slider_input:before {
                  -webkit-transform: translateX(26px);
                  -ms-transform: translateX(26px);
                  transform: translateX(26px);
                  left: -10px;
                background-color: #E97118;
                }

                /* Rounded slider_inputs */
                .slider_input.round {
                  border-radius: 34px;
                }

                .slider_input.round:before {
                  border-radius: 50%;
                }
            </style>
